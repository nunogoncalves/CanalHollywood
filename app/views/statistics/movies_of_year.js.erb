$("#moviesOfYearYear").html('<h3 class="white"><%= "#{@year} - #{@movies.count} filmes" %></h3>');
$("#moviesOfYear").html('');
<% @movies.each do |movie| %>
	$("#moviesOfYear").append('<%= j render "schedules/item", movie: movie %>');
	$("#moviesOfYear").append('<%= j render "layouts/imdb_rating_plugin" %> ');
<% end %>
(function(d,s,id) {
	var js,
			stags = d.getElementsByTagName(s)[0];
			if (d.getElementById(id)) {
				return;
			}
			js = d.createElement(s);
			js.id = id;
			js.src = "http://g-ec2.images-amazon.com/images/G/01/imdb/plugins/rating/js/rating.min.js";
			stags.parentNode.insertBefore(js,stags);
		})(document,'script','imdb-rating-api');